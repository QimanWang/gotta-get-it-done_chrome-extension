{
  "name": "Gotta Get It Done",
  "version":"0.4",
  "description": "This extension is to help you keep track of things that you want to get done in the near future.",
  "icons":{
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": ["management","cookies"],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"

}